<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IsPackable>true</IsPackable>
  </PropertyGroup>
  
  <ItemGroup>
    <Compile Remove="Properties\**" />
    <EmbeddedResource Remove="Properties\**" />
    <None Remove="Properties\**" />
  </ItemGroup>
  
   <PropertyGroup>
    <PackageId>WebEngine</PackageId>
    <Authors>Venu</Authors>
    <Company>AadyaIT</Company>
    <PackageDescription>Web Engine</PackageDescription>
    <RepositoryUrl>https://github.com/venusiddoji/nettut2pack.git</RepositoryUrl>
  </PropertyGroup>
  

</Project>
